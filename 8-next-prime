public Integer nextPrime(Integer num) {
    //code here
    integer result;
    if (num <= 1){
        result = 2;
    }

    public boolean isPrime (n+1){
        for (integer i = 1; i<=n; i++){
            if(math.mod(n+1, i)==0){
                return false;

            }        
        }
        return true;
    }



    return result;
}