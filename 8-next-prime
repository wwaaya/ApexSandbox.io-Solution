public Integer nextPrime(Integer num) {
    //code here
  
    if (num <= 1){
        return 2;
    }
    

    while (!isPrime(num)){
        num++;
    }
    return num;
    

    public boolean isPrime (n+1){
        for (integer i = 2; i<=n; i++){
            if(math.mod(n+1, i)==0){
                return false;
            }        
        }
        return true;
    }



    
}