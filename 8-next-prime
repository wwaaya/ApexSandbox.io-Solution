public Integer nextPrime(Integer num) {
    //code here
    integer result;
    if (num <= 1){
        result = 2;
    }
    return result;

    while (!isPrime(num)){
        num++;
    }else{
        return num;
    }

    public boolean isPrime (n+1){
        for (integer i = 2; i<=n; i++){
            if(math.mod(n+1, i)==0){
                return false;
            }        
        }
        return true;
    }
    return num;



    
}