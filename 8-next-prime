public Integer nextPrime(Integer num) {
    //code here
  
    if (num <= 1){
        return 2;
    }
    
    num=num+1;

    while (isPrime(num)==false){
        num++;
    }
    return num;
}
    
    

 public boolean isPrime (integer n){
        for (integer i = 2; i<n; i++){
            if(math.mod(n, i)==0){
                return false;
            }        
        }
        return true;
}
